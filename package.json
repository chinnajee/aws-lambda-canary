{
  "name": "aws-lambda-canary",
  "version": "1.0.1",
  "description": "AWS lambda canary project",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "deploy": "node ./node_modules/dpl/dpl.js",
    "test-invoke":"node ./invoke.js",
    "nocov": "node ./node_modules/.bin/mocha --compilers js:babel-register test/index.test.js",
    "semistandard": "semistandard",
    "test": "semistandard && npm run coverage",
    "coverage": "./node_modules/.bin/babel-node ./node_modules/.bin/babel-istanbul cover ./node_modules/.bin/_mocha test/index.test.js --report lcovonly -- -R spec"
  },
  "files_to_deploy": [
    "package.json",
    "index.js",
    "lib/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/numo-labs/aws-lambda-canary.git"
  },
  "author": "Jimmy Ruts",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/numo-labs/aws-lambda-canary/issues"
  },
  "homepage": "https://github.com/numo-labs/aws-lambda-canary",
  "devDependencies": {
    "aws-lambda-test-utils": "^1.0.1",
    "aws-sdk": "^2.2.33",
    "aws-sdk-mock": "^1.0.1",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-istanbul": "^0.6.0",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.5.1",
    "code": "^1.4.0",
    "css-loader": "^0.23.1",
    "dpl": "^1.0.5",
    "enzyme": "^1.5.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-install": "^0.6.0",
    "gulp-zip": "^3.0.2",
    "html-webpack-plugin": "^2.8.1",
    "mocha": "^2.4.5",
    "on-build-webpack": "^0.1.0",
    "pre-commit": "^1.1.2",
    "react-addons-test-utils": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "run-sequence": "^1.1.5",
    "semistandard": "^7.0.5",
    "simple-mock": "^0.6.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "async": "^1.5.2",
    "aws-lambda-helper": "^1.0.2",
    "babel-runtime": "^6.3.19",
    "graphql": "^0.4.14",
    "request": "^2.67.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "starwars": "^1.0.0",
    "lodash": "^4.3.0",
    "tv4": "^1.2.7",
    "uuid": "^2.0.1"
  },
  "semistandard": {
    "globals": [
      "describe",
      "it"
    ],
    "rules": {
      "no-multi-spaces": [
        1,
        {
          "exceptions": {
            "VariableDeclaration": true
          }
        }
      ],
      "brace-style": 0,
      "no-trailing-spaces": 0
    }
  }
}
